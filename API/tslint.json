{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"trailing-comma": [
			false
		],
		"quotemark": [
			true,
			"single",
			"avoid-escape",
			"avoid-template"
		],
		"indent": [
			true,
			"tabs",
			2
		],
		"eofline": false,
		"semicolon": [
			true,
			"always"
		],
		"no-console": false,
		"curly": false,
		"variable-name": {
			"options": [
				"allow-snake-case",
				"allow-pascal-case"
			]
		},
		"object-literal-sort-keys": false,
		"max-line-length": [
			true,
			140
		],
		"member-access": false,
		"ordered-imports": false
	},
	"rulesDirectory": []
}